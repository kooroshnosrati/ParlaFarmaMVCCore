@model ParlaFarmaMVCCore.Models.Dictionary

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Areas/CPanel/Views/Shared/_Layout.cshtml";
}

<h1>Edit</h1>

<h4>Dictionary</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit" asp-controller="Cpanel/Dictionaries">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="EN" class="control-label"></label>
                <input asp-for="EN" class="form-control" />
                <span asp-validation-for="EN" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AZ" class="control-label"></label>
                <input asp-for="AZ" class="form-control" />
                <span asp-validation-for="AZ" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="RU" class="control-label"></label>
                <input asp-for="RU" class="form-control" />
                <span asp-validation-for="RU" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index" asp-controller="Cpanel/Dictionaries">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
